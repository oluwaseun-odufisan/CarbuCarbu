{
  "expo": {
    "name": "CarbuCarbu",
    "slug": "CarbuCarbu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "carbucarbu",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.gif",
      "resizeMode": "contain",
      "backgroundColor": "#8A00C4"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://carbucarbu.dev/"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
